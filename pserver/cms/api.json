{
	"contenttypes": {
		"plone.dexterity.interfaces.IDexterityContent": {
            "endpoints": {
                "@components": {
                    "GET": {
                        "factory": "pserver.cms.services.components.ComponentsGET",
                        "permission": "plone.ViewContent"
                    }
                },
                "@login": {
                    "POST": {
                        "factory": "pserver.cms.services.login.Login",
                        "permission": "plone.AccessPreflight"
                    }
                }
            }
        }
	}
}